<Window x:Class="WpfAppCEFChromiumBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppCEFChromiumBrowser"
        mc:Ignorable="d"
        Title="Elchin's VIP private browser" Height="450" Width="800">
    <Grid>
        <TabControl Margin="0,46,0,0">
            <TabItem x:Name="tabItem1" Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="tabItem2" Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <TextBox x:Name="AddressBar" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="390" Height="23"/>
        <Button x:Name="GoButton" Content="Go" HorizontalAlignment="Left" Margin="411,10,0,0" VerticalAlignment="Top" Height="23" Width="30" Click="GoButton_Click"/>

    </Grid>
</Window>
